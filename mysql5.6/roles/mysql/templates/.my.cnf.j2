[client]
default-character-set = utf8
user = root
password = {{ mysql_root_password }}

[mysqld]
character-set-server=utf8
collation-server=utf8_bin
lower_case_table_names=1

sql_mode="STRICT_ALL_TABLES,NO_ZERO_DATE,NO_ZERO_IN_DATE"

[mysqld_safe]
character-set-server=utf8
collation-server=utf8_bin
lower_case_table_names=1
